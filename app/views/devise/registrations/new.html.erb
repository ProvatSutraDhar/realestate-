<h2>Sign up</h2>
<section class="form-auth text-center" >

  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= render "devise/shared/error_messages", resource: resource %>
  <!-- <img class="mb-4" src="" alt="" width="72" height="72"> -->
        <%= f.text_field :first_name,  autocomplete: "first_name", id: "inputFirstName", placeholder: "First Name", class: "form-control", autofocus: true,  required:"" %>
        <%= f.text_field :last_name,  autocomplete: "last_name", id: "inputLastName", placeholder: "Last Name", class: "form-control",  required:"" %>
        <%= f.email_field :email,  autocomplete: "email", id: "inputEmail", placeholder: "Email address", class: "form-control",  required:"" %>
        <%= f.password_field :password, id:"inputPassword", autocomplete: "new-password", class:"form-control", placeholder:"Password", required:""%>
        <%= f.password_field :password_confirmation, id:"inputPassword", autocomplete: "new-password", class:"form-control", placeholder:"Confirm password", required:""%>
        <%= f.submit "Sign up", class: "btn btn-lg btn-primary btn-block" %>
        <% end %>

    <%= render "devise/shared/links" %>
</section>
